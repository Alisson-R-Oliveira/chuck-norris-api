{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "build/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["build/**"] }
      },
      {
        "src": "client/build/**",
        "use": "@vercel/static"
      }
    ],
    "redirects": [
      {
        "source": "/jokes/(.*)",
        "destination": "build/index.js"
      },
      {
        "source": "/",
        "destination": "client/build/index.html"
      },
      {
        "source": "/(.+)",
        "destination": "client/build/index.html"
      }
    ]
}