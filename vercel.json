{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "client/build/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
  }