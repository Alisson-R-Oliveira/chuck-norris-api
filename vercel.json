{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "build/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["build/**"] }
      },
      {
        "src": "client/build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/jokes/(.*)",
        "dest": "build/index.js"
      },
      {
        "src": "/",
        "dest": "client/build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "client/build/index.html"
      }
    ]
}