{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@now/node",
        "config": { "includeFiles": ["dist/**"] }
      },
      {
        "src": "client/build/**",
        "use": "@now/static",
        "config": { "includeFiles": ["client/build/**"] }
      }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "client/build/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
  }